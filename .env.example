###############################################
# Environment Variables Example
# Copy this file to .env.local and fill values.
###############################################

# === Core AI Providers ===
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o-mini
GOOGLE_GEMINI_API_KEY=your_gemini_api_key_here
GOOGLE_GEMINI_MODEL=models/gemini-2.0-flash
ANTHROPIC_API_KEY=your_anthropic_api_key_here
COHERE_API_KEY=your_cohere_api_key_here
GROQ_API_KEY=your_groq_api_key_here

# === Search Provider APIs ===
SERP_API_KEY=your_serp_api_key_here
BRAVE_API_KEY=your_brave_api_key_here

# === Financial / Market Data ===
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here
FINNHUB_API_KEY=your_finnhub_api_key_here
RAPIDAPI_KEY=your_rapidapi_api_key_here

# === News APIs ===
NEWS_API_KEY=your_news_api_key_here

# === Vector Database (Pinecone) ===
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_ENVIRONMENT=us-west4-gcp
PINECONE_INDEX=research-agent-index

# === Redis / Caching Layer ===
REDIS_URL=redis://localhost:6379

# === Auth / Session (NextAuth) ===
NEXTAUTH_SECRET=choose-a-long-random-string
NEXTAUTH_URL=http://localhost:3000

# === Observability / Telemetry ===
LOG_LEVEL=info
TELEMETRY_ENABLED=true

# === Rate Limiting ===
RATE_LIMIT_WINDOW_SECONDS=60
RATE_LIMIT_MAX_REQUESTS=120

# === Application Settings ===
APP_BRAND_NAME=ResearchAgent
APP_BASE_URL=http://localhost:3000
EXPORT_MAX_SOURCES=25
DEMO_MODE=false
ENABLE_CACHING=true
MAX_ITERATIONS=10
CACHE_TTL=3600000

# === Feature Flags ===
ENABLE_WEB_SCRAPING=true
ENABLE_FINANCIAL_ANALYSIS=true
ENABLE_NEWS_AGGREGATION=true
ENABLE_MULTI_LLM=true
ENABLE_RAG=true
ENABLE_EXPERIMENTAL_TOOLS=false
ENABLE_AGENT_PERFORMANCE_CHARTS=true

# === Next.js Bundle Analyzer Toggle ===
ANALYZE=false

# Add any additional integration keys below as needed.
